{
  "kind": "collectionType",
  "collectionName": "servicios",
  "info": {
    "singularName": "servicio",
    "pluralName": "servicios",
    "displayName": "servicio",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "locacion": {
      "type": "customField",
      "customField": "plugin::location-field.location"
    },
    "titulo": {
      "type": "string",
      "required": true
    },
    "descripcion": {
      "type": "text",
      "required": true
    },
    "fecha_de_incio": {
      "type": "date",
      "required": true
    },
    "fecha_fin": {
      "type": "date"
    },
    "group_size": {
      "type": "biginteger",
      "required": true
    },
    "tipo_servicio": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tipo-servicio.tipo-servicio",
      "inversedBy": "tipos"
    },
    "precio": {
      "type": "float",
      "required": true
    },
    "unidad": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::unidad.unidad",
      "inversedBy": "servicios"
    },
    "portada": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "galeria": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "url_wetravel": {
      "type": "string",
      "required": true
    },
    "incluido": {
      "type": "dynamiczone",
      "components": [
        "items.item-incluido"
      ]
    },
    "no_incluido": {
      "type": "dynamiczone",
      "components": [
        "items.item-incluido"
      ],
      "required": false
    },
    "preguntas_frecuentes": {
      "type": "dynamiczone",
      "components": [
        "preguntas-frecuentes.pregunta"
      ]
    },
    "duracion": {
      "type": "string",
      "required": true
    },
    "moneda": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::monda.monda",
      "inversedBy": "servicios"
    }
  }
}
